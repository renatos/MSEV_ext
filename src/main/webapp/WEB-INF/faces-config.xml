<?xml version="1.0" encoding="utf-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">

	<application>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>

		<locale-config>
			<default-locale>pt</default-locale>
		</locale-config>

	</application>

	<navigation-rule>

		<from-view-id>/*</from-view-id>
		<navigation-case>
			<from-outcome>consulta_vendas</from-outcome>
			<to-view-id>/pages/caixa/consulta_vendas.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>consulta_movimentacao</from-outcome>
			<to-view-id>/pages/movimentacao/consulta_movimentacao.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>consulta_movimentacao_no_ano</from-outcome>
			<to-view-id>/pages/movimentacao/movimentacao_no_ano.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>